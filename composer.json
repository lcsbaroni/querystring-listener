{
    "name": "dafiti/querystring-listener",
    "description": "Dafiti Query String Listener",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dafiti",
        "querystring",
        "listener",
        "silex",
        "symfony"
    ],
    "authors": [
        {
            "name": "Fabio da Silva Ribeiro",
            "email": "fabio.ribeiro@dafiti.com.br"
        },
        {
            "name": "Luis Carlos Dias Baroni",
            "email": "luis.baroni@dafiti.com.br"
        }
    ],
    "support": {
        "issue": "https://github.com/dafiti/querystring-listener/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "silex/silex": "~1.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "fabpot/php-cs-fixer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Dafiti\\Silex\\": "src/"
